<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <AssemblyName>Richasy.ReaderKernel.Connectors.Rss</AssemblyName>
    <RootNamespace>$(AssemblyName)</RootNamespace>
    <LangVersion>latest</LangVersion>
    <VersionSuffix>alpha</VersionSuffix>
    <NoWarn>$(NoWarn);NU1507;CS1591</NoWarn>
    <Platforms>AnyCPU</Platforms>
    <IsAotCompatible>true</IsAotCompatible>
  </PropertyGroup>

  <PropertyGroup>
    <!-- NuGet Package Settings -->
    <Title>阅读核心 - RSS连接器</Title>
    <Description>用于获取不同RSS服务订阅的阅读核心子模块</Description>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\ReaderKernel.Abstractions\ReaderKernel.Abstractions.csproj" />
    <ProjectReference Include="..\..\ReaderKernel.Core\ReaderKernel.Core.csproj" />
    <ProjectReference Include="..\..\Toolkits\Toolkits.Feed\Toolkits.Feed.csproj" />
  </ItemGroup>

</Project>
